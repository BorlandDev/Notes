<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_application"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="screens.SplashFragment"
        android:label="SplashFragment" >
        <action
            android:id="@+id/action_splashFragment_to_noteListFragment3"
            app:destination="@id/noteListFragment3"
            app:popUpTo="+id/nav_graph_application"
            app:popUpToInclusive="true"
            />
    </fragment>
    <fragment
        android:id="@+id/noteListFragment3"
        android:name="com.borlanddev.notes.controller.NoteListFragment"
        android:label="NoteListFragment" >
        <action
            android:id="@+id/action_noteListFragment3_to_noteDetailsFragment3"
            app:destination="@id/noteDetailsFragment3" />
    </fragment>
    <fragment
        android:id="@+id/noteDetailsFragment3"
        android:name="com.borlanddev.notes.controller.NoteDetailsFragment"
        android:label="NoteDetailsFragment" >
        <action
            android:id="@+id/action_noteDetailsFragment3_to_noteListFragment3"
            app:destination="@id/noteListFragment3" />
    </fragment>
</navigation>